{
  
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://chat.phuongmychi.vn"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Powered-By",
          "value": "Phuong My Chi Entertainment Co.,Ltd"
        },
        {
          "key": "X-Server",
          "value": "Phuong My Chi Entertainment"
        },
        {
          "key": "Development-By",
          "value": "Phuong My Chi Entertainment Co.,Ltd"
        }
      ]
    }
  ]
}
